#include <stdio.h>

int main() {
    int a, b, c;
    printf("Enter values for a, b, and c: ");
    scanf("%d %d %d", &a, &b, &c);

    int exp1 = a * b + c;
    int exp2 = a * b + c;

    printf("Expression 1: %d * %d + %d = %d\n", a, b, c, exp1);
    printf("Expression 2: %d * %d + %d = %d\n", a, b, c, exp2);

    if (exp1 == exp2) {
        printf("Common subexpression eliminated.\n");
    } else {
        printf("Common subexpression not eliminated.\n");
    }

    return 0;
}
